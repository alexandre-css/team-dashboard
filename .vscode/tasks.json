{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "start-react-dev-server",
            "command": "npm",
            "args": [
                "start"
            ],
            "options": {
                "cwd": "${workspaceFolder}/team-dashboard"
            },
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^$"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "Starting the development server",
                    "endsPattern": "webpack compiled|Failed to compile"
                }
            }
        }
    ]
}